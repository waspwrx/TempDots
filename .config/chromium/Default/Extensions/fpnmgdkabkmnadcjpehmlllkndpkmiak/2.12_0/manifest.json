{
   "background": {
      "persistent": true,
      "scripts": [ "scripts/background.js" ]
   },
   "browser_action": {
      "default_icon": "images/icon.png",
      "default_popup": "index.html"
   },
   "description": "Reduce annoying 404 pages by automatically checking for an archived copy in the Wayback Machine.",
   "homepage_url": "https://archive.org/",
   "icons": {
      "48": "images/icon.png",
      "96": "images/icon@2x.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Wayback Machine",
   "permissions": [ "notifications", "tabs", "activeTab", "storage", "webRequest", "webRequestBlocking", "contextMenus", "http://*/*", "https://*/*", "*://*/*", "\u003Call_urls>" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.12",
   "web_accessible_resources": [ "images/insetIcon.svg", "images/close.svg", "images/logo.gif", "images/logo-animate.svg", "scripts/client.js", "scripts/RTcontent.js", "scripts/sequences.js", "scripts/d3.js", "scripts/radial-tree.umd.js", "css/RTstyle.css", "css/sequences.css", "css/googlestyle.css", "css/radial-tree.css" ]
}
